<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Menu-->
    <SolidColorBrush x:Key="Menu_Background" Color="#FF2B2B2B"/>
    <SolidColorBrush x:Key="Menu_BackgroundHover" Color="#FF597FB6"/>
    <SolidColorBrush x:Key="Menu_BackgroundPressed" Color="#FF263451"/>
    <SolidColorBrush x:Key="Menu_Border" Color="#00FFFFFF"/>
    <SolidColorBrush x:Key="Menu_BorderHover" Color="#00FFFFFF"/>
    <SolidColorBrush x:Key="Menu_BorderPressed" Color="#00FFFFFF"/>
    <SolidColorBrush x:Key="Menu_Foreground" Color="#FFFFFFFF"/>
