    <SolidColorBrush x:Key="ListBox_BackgroundHover" Color="#FF263451"/>
    <SolidColorBrush x:Key="ListBox_BorderPressed" Color="#00FFFFFF"/>

    <!--MenuItem-->
    <SolidColorBrush x:Key="MenuItem_Background" Color="#FF263451"/>
